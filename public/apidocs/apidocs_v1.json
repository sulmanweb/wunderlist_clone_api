    "definitions": {
        "signuppartial": {
            "title": "SignUpPartial",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "username": {
                    "type": "string",
                    "pattern": "\\A(\\w){1,15}\\z"
                },
                "email": {
                    "type": "string",
                    "pattern": "\\A(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))\\z"
                },
                "password": {
                    "type": "string",
                    "minLength": 8,
                    "maxLength": 72,
                    "pattern": "\\A(?=.*[0-9])(?=.*[!@#\\$%\\^&\\*])"
                }
            },
            "required": [
                "username",
                "email",
                "password"
            ],
            "example": {
                "name": "Sulman Baig",
                "username": "sbaigweb",
                "email": "sulman.baig@synnapps.com",
                "password": "abcd@1234"
            }
        },
        "user-self": {
            "title": "User Self",
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "required": [
                "id",
                "email",
                "username",
                "created_at"
            ],
            "example": {
                "id": 4,
                "name": "Sulman Baig",
                "username": "sbaigweb",
                "email": "sulman.baig@synnapps.com",
                "created_at": "2018-01-16T10:20:23.000Z"
            }
        },
        "signinpartial": {
            "title": "SignInPartial",
            "type": "object",
            "properties": {
                "auth": {
                    "type": "string",
                    "description": "can be user's `email` or `username` in here"
                },
                "password": {
                    "type": "string"
                }
            },
            "required": [
                "auth",
                "password"
            ]
        },
        "session": {
            "title": "Session",
            "type": "object",
            "properties": {
                "sid": {
                    "type": "integer"
                },
                "utoken": {
                    "type": "string",
                    "minLength": 64,
                    "maxLength": 64
                },
                "last_used_at": {
                    "type": "string",
                    "format": "date-time"
                },
                "created_at": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "required": [
                "sid",
                "utoken",
                "last_used_at",
                "created_at"
            ],
            "example": {
                "utoken": "GupXkUkJ2aNnXqgRcnP1LM7kjkd19Zsvv9HeA5bSCWQ9ipxQqgVEPvqbdkrkzS8u",
                "last_used_at": "2018-01-16T10:26:04.000Z",
                "created_at": "2018-01-16T10:26:04.000Z",
                "sid": 1
            }
        },
        "listpartial": {
            "title": "ListPartial",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            },
            "required": [
                "name"
            ]
        },
        "list": {
            "title": "List",
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "user_id": {
                    "type": "integer"
                },
                "created_at": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "required": [
                "id",
                "name",
                "user_id",
                "created_at"
            ]
        },
        "taskpartial": {
            "title": "TaskPartial",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "status": {
                    "type": "integer",
                    "default": 0,
                    "description": "0 => todo\n\n1 => done",
                    "enum": [
                        "todo",
                        "done"
                    ],
                    "minimum": 0,
                    "maximum": 1
                }
            },
            "required": [
                "name",
                "status"
            ]
        },
        "task": {
            "title": "Task",
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "status": {
                    "type": "integer",
                    "default": "todo",
                    "enum": [
                        "todo",
                        "done"
                    ]
                },
                "list_id": {
                    "type": "integer"
                },
                "created_at": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "required": [
                "id",
                "name",
                "status",
                "list_id",
                "created_at"
            ]
        }
